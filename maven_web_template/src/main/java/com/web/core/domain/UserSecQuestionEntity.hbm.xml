<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.web.core.domain.UserSecQuestionEntity" table="user_sec_question" schema="webconsole">
        <id name="id" column="id"/>
        <property name="userId" column="userId"/>
        <property name="secQuestion" column="sec_question"/>
        <property name="secAnswer" column="sec_answer"/>
    </class>
</hibernate-mapping>